{
    "name": "cdk8s-aws-cdk",
    "version": "0.0.0",
    "main": "main.js",
    "types": "main.ts",
    "license": "Apache-2.0",
    "private": true,
    "scripts": {
      "compile": "tsc",
      "watch": "tsc -w",
      "test": "echo ok",
      "clean": "rm -rf cdk.out cdktf.out dist",
      "synth": "npx ts-node poc.ts",
      "deploy": "cdk -a 'npx ts-node poc.ts' deploy --require-approval never && cdktf deploy --auto-approve && npm run synth",
      "destroy": "cdk -a 'npx ts-node poc.ts' destroy --require-approval never && cdktf destroy --auto-approve"
    },
    "dependencies": {
      "cdk8s": "./cdk8s.v0.0.0.jsii.tgz",
      "cdk8s-plus-25": "^2.5.8",
      "aws-cdk-lib": "^2.69.0",
      "cdktf": "^0.15.5",
      "@cdktf/provider-aws": "^12.0.11",
      "constructs": "^10.1.291"
    },
    "devDependencies": {
      "aws-cdk": "^2.69.0",
      "cdktf-cli": "^0.15.5",
      "cdk8s-cli": "^2.2.3",
      "ts-node": "^10.9.1"
    }
  }