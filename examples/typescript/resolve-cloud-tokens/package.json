{
  "name": "cdk8s-aws-cdk",
  "version": "0.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "test": "echo ok",
    "clean": "rm -rf cdk.out cdktf.out dist",
    "install-cdk8s-core": "cd ../../../../cdk8s-core && yarn compile && yarn package:js && mv ./dist/js/cdk8s@0.0.0.jsii.tgz ./../cdk8s/examples/typescript/resolve-cloud-tokens/cdk8s.v0.0.0.jsii.tgz",
    "synth:awscdk-app": "cdk -a 'npx ts-node ./apps/awscdk-app.ts' synth",
    "deploy:awscdk-app": "cdk -a 'npx ts-node ./apps/awscdk-app.ts' deploy --require-approval never",
    "destroy:awscdk-app": "cdk -a 'npx ts-node ./apps/awscdk-app.ts' destroy --require-approval never",
    "synth:cdktf-app": "cdktf synth -a 'npx ts-node ./apps/cdktf-app.ts'",
    "deploy:cdktf-app": "cdktf deploy -a 'npx ts-node ./apps/cdktf-app.ts' --auto-approve",
    "destroy:cdktf-app": "cdktf destroy -a 'npx ts-node ./apps/cdktf-app.ts' --auto-approve",
    "synth:cdk8s-app": "cdk8s synth -a 'npx ts-node ./apps/cdk8s-app.ts'"
  },
  "dependencies": {
    "cdk8s": "./cdk8s.v0.0.0.jsii.tgz",
    "cdk8s-plus-25": "^2.5.8",
    "aws-cdk-lib": "^2.69.0",
    "cdktf": "^0.15.5",
    "@cdktf/provider-aws": "^12.0.11",
    "constructs": "^10.1.291"
  },
  "devDependencies": {
    "aws-cdk": "^2.69.0",
    "cdktf-cli": "^0.15.5",
    "cdk8s-cli": "^2.2.3",
    "@aws-sdk/client-cloudcontrol": "^3.282.0",
    "@aws-sdk/client-cloudformation": "^3.299.0",
    "ts-node": "^10.9.1"
  }
}