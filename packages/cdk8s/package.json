{
  "name": "@awslabs/cdk8s",
  "version": "0.0.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "description": "Cloud Development Kit for Kubernetes",
  "scripts": {
    "build": "jsii",
    "watch": "jsii -w",
    "package": "jsii-pacmak",
    "test": "jest"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "short",
    "targets": {
      "java": {
        "package": "org.cdk8s",
        "maven": {
          "groupId": "org.cdk8s",
          "artifactId": "cdk8s"
        }
      },
      "python": {
        "distName": "cdk8s",
        "module": "cdk8s"
      },
      "dotnet": {
        "namespace": "Org.Cdk8s",
        "packageId": "Org.Cdk8s"
      }
    }
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/awslabs/cdk8s.git"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/core": "^1.24.0",
    "@aws-cdk/cx-api": "^1.24.0",
    "@types/jest": "^25.1.2",
    "@types/node": "13.7.1",
    "@types/yaml": "^1.2.0",
    "jest": "^25.1.0",
    "jsii": "^0.22.0",
    "jsii-pacmak": "^0.22.0",
    "json-schema-to-typescript": "^8.0.1",
    "typescript": "^3.7.5"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js"
    ]
  },
  "bundledDependencies": [
    "yaml"
  ],
  "dependencies": {
    "yaml": "^1.7.2"
  },
  "peerDependencies": {
    "@aws-cdk/core": "^1.24.0",
    "@aws-cdk/cx-api": "^1.24.0"
  },
  "stability": "experimental"
}
